{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "Shopping list built with the PERN stack",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf build",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "start": "node build/server.js",
    "server": "nodemon babel/node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Bosiipo",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.1",
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "nodemon": "^2.0.2",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": "11.13.0",
    "npm": "6.13.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7"
  }
}
